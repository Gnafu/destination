<?xml version="1.0" encoding="UTF-8"?>
<roleRegistry version="1.0" xmlns="http://www.geoserver.org/security/roles">
    <roleList>
        <role id="ADMIN"/>
        <role id="BASEUSER_SIIG"/>
        <role id="GROUP_ADMIN"/>
        <role id="MAJORUSER_SIIG"/>
        <role id="ROLE_ADMIN"/>
        <role id="ROLE_BASE"/>
        <role id="ROLE_MAJOR"/>
        <role id="ROLE_SUPER"/>
        <role id="SUPERUSER_SIIG"/>
        <role id="TEST"/>
        <role id="tomcat"/>
    </roleList>
    <userList>
        <userRoles username="admin">
            <roleRef roleID="ADMIN"/>
            <roleRef roleID="ROLE_ADMIN"/>
        </userRoles>
        <userRoles username="base">
            <roleRef roleID="BASEUSER_SIIG"/>
            <roleRef roleID="ROLE_BASE"/>
        </userRoles>
        <userRoles username="super">
            <roleRef roleID="ROLE_ADMIN"/>
            <roleRef roleID="SUPERUSER_SIIG"/>
        </userRoles>
        <userRoles username="test"/>
    </userList>
    <groupList/>
</roleRegistry>
